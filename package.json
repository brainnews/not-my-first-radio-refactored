{
  "name": "not-my-first-radio-refactored",
  "version": "2.0.0",
  "description": "Algorithm-free internet radio player - refactored with modern architecture",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "pages:build": "npm run type-check && npm run lint && npm run build",
    "pages:deploy": "wrangler pages deploy dist --project-name not-my-first-radio",
    "pages:deploy:preview": "wrangler pages deploy dist --project-name not-my-first-radio --env preview",
    "cap:build": "npm run build && npx cap sync",
    "cap:android": "npm run cap:build && npx cap run android",
    "cap:ios": "npm run cap:build && npx cap run ios"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "jsqr": "^1.4.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-legacy": "^4.1.0",
    "eslint": "^8.45.0",
    "terser": "^5.19.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-pwa": "^0.16.0",
    "vitest": "^0.34.0",
    "workbox-window": "^7.0.0"
  },
  "keywords": [
    "radio",
    "streaming",
    "algorithm-free",
    "web-app",
    "pwa"
  ],
  "author": "Miles Gilbert",
  "license": "MIT"
}
